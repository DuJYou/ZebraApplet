/* pages/settings/settings.wxss */
.header {
    background: #fff;
    position: fixed;
    top: 0;
    z-index: 100;
    width: 100%;
    height: 44Px;
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 0 18px;
    box-sizing: border-box;
    border-bottom: 1Px solid #E2E2E2;
    border-radius: 4px;
    text-align: center;
}

.return {
    position: absolute;
    left: 18px;
    color: #666464;
    font-size: 24px;

}

.title {
    font-size: 15px;
}

.ico {
    padding-top: 44px;
    display: flex;
    width: 100%;
    height: 151px;
    background: #fff;
    border-bottom: 1px solid #f0f0f0;
    flex-direction: column;
    justify-content: center;
    align-items: center;

}

.icoImg {
    width: 44px;
    height: 44px;
    margin: 0 0 12px;
}
.icoText{
    font-family: PingFangSC-Medium;
    font-weight: 500;
    font-size: 14px;
    color: #333333;
    letter-spacing: 0;
    margin-bottom: 6px;
}

.list{
    background: #fff;
    border-radius: 1.8px;
    padding: 0 18px;
    margin-bottom: 9px;
}
.item{
    height: 50px;
    width: 100%;
    display: flex;
    justify-content: space-between;
    position: relative;
    border-bottom: 1Px solid #E2E2E2;
    line-height: 50px;
}
.items{
    display: flex;
    flex-direction: row;
}
.listImg{
    display: inline-block;
    width: 18px;
    height: 18px;
    vertical-align: text-top;
    margin: 18px;
}
.but{
    margin: 0 18px;
    margin-top:36px;
}
.out{
    width: 100%;
    height: 45px;
    line-height: 45px;
    border: 1px solid #C14374;
    border-radius: 3px;
    background: transparent;
    font-size: 12px;
    font-weight: 500;
    color: #C14374;
    letter-spacing: 0;
    padding: 0;
}