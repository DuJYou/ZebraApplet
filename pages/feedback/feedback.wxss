/* pages/feedback/feedback.wxss */
.header {
    background: #fff;
    position: fixed;
    top: 0;
    z-index: 100;
    width: 100%;
    height: 44Px;
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 0 18px;
    box-sizing: border-box;
    border-bottom: 1Px solid #E2E2E2;
    border-radius: 4px;
    text-align: center;
}

.return {
    position: absolute;
    left: 18px;
    color: #666464;
    font-size: 24px;

}

.title {
    font-size: 15px;
}
.feedbackContent{
    padding: 18Px;
    padding-top: 44px;
    text-align: left;
    font-size: 12px;
}
.list{
    margin-bottom: 36px;
}
.listTitle{
    color: #333333;
    font-size: 13px;
    letter-spacing: 0.75px;
    font-weight: 500;
    margin-bottom: 9px;
}
.inp{
    border-bottom: 1px solid #E1E1E1;
    padding: 15px 0;
    height: 100%;
    width: 100%;
}
.but{
    width: 340px;
    color: #fff;
    font-size: 15px;
    height: 44px;
    background: #C04374;
    line-height: 44px;
    text-align: center;
}