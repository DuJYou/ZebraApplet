<!-- pages/personal/personal.wxml -->
<view style="background:#f5f5f5">
    <view class="bm-personal-top">
        <view class="bm-personal-info">
            <view class="bm-personal-info-unsign">
                <text class="bm-personal-info-unsign-text">Hi,  终于等到你！</text>
                <text class="bm-personal-info-unsign-info">让每个家庭享受省心、精彩的旅行体验</text>
                <button class="bt">登录/注册</button>
            </view>
            <view class="myList">
                <view class="myCoupon">
                    <text>---</text>
                    <text class="bord">优惠卷</text>
                </view>
                <view class="myintegral">
                    <text>---</text>
                    <text class="bord">我的积分</text>
                </view>
                <view class="myCollect">
                    <text>---</text>
                    <text>我的收藏</text>
                </view>
            </view>
        </view>
        <view class="bm-personal-main-insert" hover-class="none" hover-stop-propagation="false">
            <view class="our">
                <view class="myOrder">
                    <view class="ourI">
                        <image class="ourImg" src="data:image/svg+xml;base64,"></image>
                    </view>
                    <text class="ourText" selectable="false" space="false" decode="false">
                        我的订单
                    </text>
                </view>
                <view class="myJourney">
                    <view class="ourI">
                        <image class="ourImg" src="data:image/svg+xml;base64,"></image>
                    </view>
                    <text class="ourText" selectable="false" space="false" decode="false">
                        我的行程
                    </text>
                </view>
                <view class="jfShop">
                    <view class="ourI">
                        <image class="ourImg" src="data:image/svg+xml;base64,"></image>
                    </view>
                    <text class="ourText" selectable="false" space="false" decode="false">
                        积分商城
                    </text>
                </view>
            </view>
        </view>
    </view>
    <view class="list_item">
        <view class="item">
            <image class="itemImg" src="data:image/svg+xml;base64," mode="aspectFit|aspectFill|widthFix" lazy-load="false" binderror="" bindload=""></image>
            <text class="itemText" selectable="false" space="false" decode="false">常用出行人信息</text>
            <text class="itemNext" selectable="false" space="false" decode="false">></text>
        </view>
        <view class="item" bindtap="onClickFeeback">
            <image class="itemImg" src="data:image/svg+xml;base64," mode="aspectFit|aspectFill|widthFix" lazy-load="false" binderror="" bindload=""></image>
            <text class="itemText" selectable="false" space="false" decode="false">用户反馈</text>
            <text class="itemNext" selectable="false" space="false" decode="false">></text>
        </view>
        <view class="item" bindtap="onClickSet">
            <image class="itemImg" src="data:image/svg+xml;base64," mode="aspectFit|aspectFill|widthFix" lazy-load="false" binderror="" bindload=""></image>
            <text class="itemText" selectable="false" space="false" decode="false">设置</text>
            <text class="itemNext" selectable="false" space="false" decode="false">></text>
        </view>
    </view>
</view>