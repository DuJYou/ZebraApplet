/* pages/product/product.wxss */
.imageWrap{
    width: 100%;
    height: 173px;
    position: relative;
}
.imageWrap image{
    width: 100%;
    height: 173px;
}
.TabBotton{
    position: absolute;
    display: flex;
    bottom: 0;
    left: 0;
    width: 100%;
    padding:0 4%;
}
.TabBottonView{
    width: 46%;
    font-family: PingFangSC-Medium;
    font-weight: 600;
    letter-spacing: 0.002px;
    text-align: center;
    line-height: 50px;
    border-radius:5px;
    border-bottom: 1px solid #e1e1e1;
    position: relative;
    font-size: 12.48px;
}
